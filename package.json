{
  "name": "stylelint-suitcss",
  "version": "5.0.0",
  "description": "A collection of stylelint plugins for SUIT CSS",
  "keywords": [
    "stylelint-plugin",
    "stylelint",
    "css",
    "lint",
    "suitcss"
  ],
  "repository": "suitcss/stylelint-suitcss",
  "bugs": {
    "url": "https://github.com/suitcss/stylelint-suitcss/issues"
  },
  "homepage": "https://github.com/suitcss/stylelint-suitcss",
  "engines": {
    "node": ">=4"
  },
  "main": "index.js",
  "jest": {
    "preset": "jest-preset-stylelint",
    "testMatch": [
      "**/index.test.js",
      "**/rules/**/test/*.js"
    ]
  },
  "scripts": {
    "lint": "eslint rules utils",
    "pretest": "yarn lint",
    "test": "jest "
  },
  "author": "Simon Smith",
  "license": "MIT",
  "devDependencies": {
    "jest": "^27.4.7",
    "eslint": "^8.6.0",
    "jest-preset-stylelint": "^4.2.0"
  },
  "dependencies": {
    "lodash.find": "^4.6.0",
    "postcss-selector-parser": "^6.0.8",
    "stylelint": "^14.2.0"
  }
}
